       "icons": "flight_takeoff",
        "displayName": "Visa on Arrival",
        "chooseCaption":"Do you want Visa on Arrival?"
     },
     "forex":{
       "icons":"monetization_on",
        "displayName": "Foreign Exchange",
        "chooseCaption": "Do you want Forex service?"
     }
   },
   "mutuallyExclusive": {
     "mode":{
       "displayName":"Mode of travel",
        "chooseCaption": "Choose Travel Mode.",
       "servicesDetails": {
         "flight": {
           "icons": "flight",
           "displayName": "Flight",
           "selectedFieldDetails":{
             "preferences": {
               "class":"Class",
               "nonStop": "Non-Stop",
               "meals": "Meals",
               "extraBaggage": "Extra Baggage"
             }
           }
          },
         "train": {
           "icons":"train",
           "displayName":"Train",
           "selectedFieldDetails":{
             "preferences": {
               "class":"Class"
             }
           }
         },
         "bus":{
           "icons":"directions_bus",
           "displayName":"Bus",
           "selectedFieldDetails":{
             "preferences": {
               "class":"Class",
               "seatType": "Seat Type"
             }
           }
         }
       }
     }
   }
 },
  "essential": {
    "noDependencyData": {
      "travelStartDate": {
        "mandatory": "true",
        "displayName": "Travel Start Date",
        "id": "travelStartDate",
        "type": "date"
      }
    },
    "modesToSelectTheServices": {

      "mode": {
        "mandatory": true,
        "displayName": "Select the mode",
        "id": "mode",
        "type": "singleSelect",
        "specificAttr": {
          "domainList": {"flight":"Flight", "bus":"Bus", "train":"Train"}
        },
        "javascriptDataType": "String"
      },
      "extraAddOnsServices": {
        "mandatory": false,
        "displayName": "Extra Add-on Services",
        "id": "extraAddOnsServices",
        "type": "multiSelect",
        "specificAttr": {
          "domainList": ["visaOnArrival", "forex"]
        },
        "javascriptDataType": "Array"
      }
    }
  },
  "services": {
    "flight": {
      "travelStartDate": {
        "mandatory": true,
        "displayName": "Travel Start Date",
        "id": "travelStartDate",
        "type": "date",
        "data-reference": "essential.travelStartDate"
      },
      "class": {
        "mandatory": true,
        "displayName": "Class",
        "id": "class",
        "type": "singleSelect",
        "specificAttr": {
          "domainList": ["Bussiness", "Economy"]
        }

      },
      "numberOfHops": {
        "mandatory": true,
        "displayName": "Number of Hops",
        "id": "numberOfHops",
        "type": "slider",
        "specificAttr": {
          "min": 0,
          "max": 15
        }
      },
      "departureTime": {
        "mandatory": false,
        "displayName": "Departure Time",
        "id": "departureTime",
        "type": "time"
      },
      "airlines": {
        "mandatory": false,
        "displayName": "Airlines",
        "id": "airlines",
        "type": "singleSelect",
        "specificAttr": {
          "domainList": ["Air Asia", "Air Costa", "Jet"]
        }

      }
    }
  }

}
